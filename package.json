{
  "name": "katalon-cli",
  "version": "v1.0.3",
  "description": "",
  "main": "cli.js",
  "scripts": {
    "start": "node cli.js start-agent",
    "beforebuild": "",
    "build": "npm run build:pkg && npm run afterbuild",
    "build:serv": "npm run beforebuild && npm run build:pkg && npm run afterbuild",
    "build:pkg": "pkg --targets node10-linux-x64,node10-macos-x64,node10-win-x64,node10-win-x86 --out-path bin cli.js",
    "afterbuild": "npm run copyConfig && npm run copyService && npm run copyStart",
    "copyConfig": "cpy agentconfig_template bin --rename agentconfig",
    "copyService": "cpy nssm.exe service.bat service.sh bin",
    "copyStart": "cpy start.bat start.sh bin",
    "release": "release-it --ci"
  },
  "dependencies": {
    "archiver": "^3.1.1",
    "commander": "^3.0.0",
    "cpy-cli": "^2.0.0",
    "decompress": "^4.2.0",
    "find": "^0.3.0",
    "fs-extra": "^8.1.0",
    "glob": "^7.1.4",
    "ini": "^1.3.4",
    "ip": "^1.1.5",
    "lodash": "^4.17.15",
    "log4js": "^5.0.0",
    "moment": "^2.24.0",
    "properties": "^1.2.1",
    "request": "^2.72.0",
    "rsvp": "^4.8.5",
    "tmp": "^0.1.0",
    "triple-beam": "^1.3.0",
    "url-join": "^4.0.1",
    "uuid": "^3.3.2",
    "winston": "^3.2.1",
    "winston-transport": "^4.3.0"
  },
  "author": "katalon",
  "license": "ISC",
  "devDependencies": {
    "eslint": "^6.1.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.18.2",
    "pkg": "4.4.0",
    "release-it": "^12.3.5"
  }
}

{
  "name": "katalon-cli",
  "version": "1.0.0",
  "description": "",
  "main": "cli.js",
  "scripts": {
    "beforebuild": "",
    "build": "npm run build:pkg && npm run afterbuild",
    "build:serv": "npm run beforebuild && npm run build:pkg && npm run afterbuild",
    "build:pkg": "pkg --targets node10-linux-x64,node10-linux-x86,node10-macos-x64,node10-win-x64,node10-win-x86 --out-path bin cli.js",
    "afterbuild": "npm run copyConfig && npm run copyService",
    "copyConfig": "cpy agentconfig_template bin --rename agentconfig",
    "copyService": "cpy nssm.exe service.bat service.sh bin"
  },
  "dependencies": {
    "archiver": "^3.0.0",
    "commander": "^2.19.0",
    "cpy-cli": "^2.0.0",
    "decompress": "^4.2.0",
    "find": "^0.3.0",
    "fs-extra": "^7.0.1",
    "glob": "^7.1.3",
    "ini": "^1.3.4",
    "ip": "^1.1.5",
    "lodash": "^4.13.1",
    "log4js": "^4.0.2",
    "moment": "^2.24.0",
    "properties": "^1.2.1",
    "request": "^2.72.0",
    "rsvp": "^4.8.4",
    "tmp": "0.0.33",
    "url-join": "^4.0.0",
    "uuid": "^3.3.2",
    "winston": "^3.2.1"
  },
  "author": "katalon",
  "license": "ISC",
  "devDependencies": {
    "pkg": "^4.2.4"
  }
}
